{
  "name": "reactstarter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "Apache-2.0",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.dev.js --mode development --progress --colors --open",
    "build": "webpack --config ./webpack.config.prod.js --mode production --progress --colors ",
    "lint": "eslint src",
    "lint:css": "stylelint \"src/**/*.css\"",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "format": "prettier --write '{.babelrc,.eslintrc,**/*.{js,css,md}}'"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-postcss": "^2.0.2",
    "eslint-config-prettier": "^2.1.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.1.1",
    "eslint-plugin-react": "^7.4.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.6",
    "prettier": "1.10.2",
    "react-hot-loader": "^4.0.0",
    "style-loader": "^0.20.3",
    "stylelint": "^8.0.0",
    "stylelint-config-prettier": "^2.1.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
